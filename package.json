{
  "name": "portfolio-roboy",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.12.2",
    "npm": ">=10.7.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "prebuild": "npm-run-all --parallel generate:sitemap optimize:images",
    "preview": "vite preview",
    "analyze": "vite build --mode analyze",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --max-warnings 0",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "test": "vitest",
    "test:coverage": "vitest run --coverage --reporter=json --reporter=lcov --reporter=text",
    "test:ci": "vitest run --coverage",
    "check": "npm-run-all check:*",
    "check:types": "tsc --noEmit",
    "check:build": "cross-env NODE_ENV=production vite build --emptyOutDir",
    "check:security": "npm audit --omit=dev",
    "check:deps": "npm outdated --long",
    "generate:sitemap": "node scripts/sitemap-generator.js",
    "optimize:images": "node scripts/image-optimizer.js",
    "upload:assets": "node scripts/cloudinary-upload.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "deploy:prod": "netlify deploy --prod",
    "deploy:preview": "netlify deploy --branch=preview",
    "healthcheck": "curl -f https://$NETLIFY_SITE_URL/.netlify/functions/healthcheck",
    "prepare": "husky install",
    "postinstall": "node scripts/postinstall.js",
    "audit:health": "node scripts/health-audit/runAudit.js",
    "audit:deps": "npm-run-all --parallel check:deps audit:health",
    "audit:full": "npm-run-all --parallel check:* audit:health"
  },
  "dependencies": {
    "@cloudinary/react": "1.13.1",
    "@cloudinary/url-gen": "1.21.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@sentry/react": "^7.101.1",
    "@sentry/tracing": "7.120.3",
    "@tanstack/react-query": "^5.34.3",
    "clsx": "2.1.1",
    "framer-motion": "11.18.2",
    "lucide-react": "0.475.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "6.29.0",
    "sharp": "^0.32.6",
    "sonner": "^1.3.1",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@radix-ui/react-dialog": "1.1.6",
    "@sentry/vite-plugin": "^2.14.2",
    "@storybook/addon-essentials": "^8.1.6",
    "@storybook/addon-interactions": "^8.1.6",
    "@storybook/addon-links": "^8.1.6",
    "@storybook/blocks": "^8.1.6",
    "@storybook/react": "^8.1.6",
    "@storybook/react-vite": "^8.1.6",
    "@storybook/test": "^8.1.6",
    "@swc/core": "^1.3.109",
    "@swc/plugin-emotion": "8.6.0",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/container-queries": "0.1.1",
    "@tailwindcss/forms": "0.5.10",
    "@tailwindcss/typography": "0.5.16",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.2.0",
    "@types/babel__core": "7.20.5",
    "@types/babel__generator": "7.6.8",
    "@types/babel__template": "7.4.4",
    "@types/babel__traverse": "7.20.6",
    "@types/doctrine": "0.0.9",
    "@types/estree": "1.0.6",
    "@types/jest": "29.5.14",
    "@types/json-schema": "7.0.15",
    "@types/lodash": "4.17.15",
    "@types/mdx": "2.0.13",
    "@types/node": "22.13.4",
    "@types/parse-json": "4.0.2",
    "@types/prop-types": "15.7.14",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/rollup": "0.54.0",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "@vitejs/plugin-react-swc": "3.8.0",
    "autoprefixer": "10.4.20",
    "caniuse-lite": "1.0.30001700",
    "cloudinary": "2.5.1",
    "cross-env": "^7.0.3",
    "cssnano": "7.0.6",
    "dotenv": "16.4.7",
    "eslint": "8.45.0",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.3",
    "husky": "^9.0.11",
    "jsdom": "26.0.0",
    "lint-staged": "^15.2.2",
    "npm-run-all": "^4.1.5",
    "postcss": "8.5.2",
    "postcss-import": "16.1.0",
    "postcss-nesting": "13.0.1",
    "postcss-preset-env": "^10.1.4",
    "prettier": "3.5.1",
    "rollup": "4.34.8",
    "rollup-plugin-visualizer": "5.14.0",
    "tailwind-merge": "^3.0.1",
    "tailwindcss": "^3.4.3",
    "terser": "5.39.0",
    "ts-node": "10.9.2",
    "typescript": "5.1.6",
    "vite": "5.4.14",
    "vitest": "1.6.1",
    "web-vitals": "^3.5.0"
  },
  "volta": {
    "node": "20.12.2",
    "npm": "10.7.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint:fix",
      "npm run check:types"
    ]
  },
  "files": [
    "dist",
    "public",
    "src",
    "types"
  ],
  "overrides": {
    "react": "^18.2.0",
    "typescript": "5.1.6"
  }
}
