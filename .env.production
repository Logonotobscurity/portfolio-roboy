# App Configuration
VITE_APP_TITLE=Portfolio Roboy
VITE_API_URL=https://rooboy.netlify.app/api
VITE_APP_ENV=production
VITE_APP_VERSION=0.1.0

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_TRACKING=true

# Sentry Configuration
VITE_ENABLE_SENTRY=true

# Build Configuration
NODE_ENV=production
CI=true
BUILD_PATH=dist
NODE_VERSION=20.x
INDEX_FILE=index.html
ERROR_FILE=index.html
ANALYZE=true

# Netlify Configuration
NETLIFY_AUTH_TOKEN=your-token-here
NETLIFY_SITE_ID=32870b7f-f27a-483e-bc41-e389092f9221

# Performance and Optimization
VITE_DISABLE_CONSOLE=true
VITE_SOURCEMAP=hidden
VITE_COMPRESS=true

# Security Headers
VITE_SECURITY_HEADERS=true
STRICT_CSP=true

# Cache Control
VITE_CACHE_CONTROL="public, max-age=31536000, immutable"
